<Window x:Class="Cvrle.DodajSastojak"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cvrle"
        mc:Ignorable="d"
        Title="Cvrle - Dodavanje sastojaka" Height="310.198" Width="481.02" Icon="cvrle.ico">
    <Window.Resources>
        <Style x:Key="SeparatorColumn" TargetType="{x:Type ColumnDefinition}">
            <Setter Property="Width" Value="10"/>
        </Style>
        <Style x:Key="SeparatorRow" TargetType="{x:Type RowDefinition}">
            <Setter Property="Height" Value="3"/>
        </Style>
    </Window.Resources>
    <Grid x:Name="grid">
        <Label x:Name="label" Content="Dodajte gramazu za sastojak:" HorizontalAlignment="Left" Height="39" Margin="96,21,0,0" VerticalAlignment="Top" Width="291" FontSize="21.333"/>
        <Grid x:Name="grid2" HorizontalAlignment="Left" Height="166" Margin="23,62,0,0" VerticalAlignment="Top" Width="429"/>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Height="23" Margin="174,250,0,0" VerticalAlignment="Top" Width="109" Click="button_Click"/>
    </Grid>
</Window>
